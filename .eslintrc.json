{    
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 8,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true,
            "experimentalObjectRestSpread": true,
            "modules": true
        }        
    },
    "rules": {
        "semi": ["error", "always"],
        "quotes": ["error", "single"],        
        "keyword-spacing": ["error", { "after": true, "before": true }]
    },
    "plugins": [
      "react",
       "babel"
    ]
}